/* components/title-bar/title-bar.wxss */
.title-bar {
  display: flex;
  justify-content: space-between;
}

.title-bar .title {
  font-size: large;
  font-weight: bolder;
}

.title-bar .more-title {
  font-size: small;
}

.title-bar .more {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.title-bar .icon {
  width: 45rpx;
  height: 45rpx;
}